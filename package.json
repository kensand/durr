{
  "name": "durr",
  "version": "0.0.3",
  "license": "MIT",
  "description": "Typescript duration models and utilities",
  "repository": {
    "url": "https://github.com/kensand/durr.git"
  },
  "author": {
    "name": "Kenneth Sanders",
    "email": "npm@kensand.net"
  },
  "private": false,
  "dependencies": {
    "typescript": "^4.9.4"
  },
  "scripts": {
    "prettier": "npx prettier --write .",
    "_build": "tsc",
    "build": "npm run _pre-build && npm run _build && npm run _post-build",
    "_post-build": "npm run babel-cjs && npm run test",
    "_pre-build": "npm run prettier",
    "test": "jest",
    "clean": "rm -rf dist",
    "pub": "npm run clean && npm run dist && npm version patch && npm publish",
    "update": "npm update --save && ((! git diff --exit-code package.json > /dev/null && git add package.json && git commit -m 'Dependency updates') || true)",
    "babel-cjs": "babel -s true -d dist/cjs dist/esm"
  },
  "eslintConfig": {},
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^16.18.10",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5",
    "prettier": "^2.8.1",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/cli": "^7.20.7"
  },
  "main": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/types",
      "import": "./dist/esm",
      "require": "./dist/cjs"
    }
  }
}
